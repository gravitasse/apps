{
  "bgpglobal": {
    "createwithdefault": {
      "default": true, 
      "type": "boolean", 
      "description": "Attribute used to tell the cli whether an object can be created withdefaultref and/or default settings.  If this is false, all attributes must be set by user in order for create to be called."
    }, 
    "required": [
      "value", 
      "cliname"
    ], 
    "type": "object", 
    "properties": {
      "commands": {
        "type": "object", 
        "properties": {
          "$ref": "file:/tmp/snaproute/cli/schema/BGPGlobalSubCmds.json"
        }
      }, 
      "cliname": {
        "default": "", 
        "type": "string", 
        "description": "name used in cli to describe bfd"
      }, 
      "help": {
        "default": "", 
        "type": "string"
      }, 
      "prompt": {
        "default": "", 
        "displayValue": false, 
        "type": "string", 
        "description": "what to display on the prompt when the user has entered a command"
      }, 
      "value": {
        "type": "object",
        "description": "this is a trigger to cli that a command needs one of the attribute values contained in the properties commands LeafMemberTemplate",
        "properties": {
          "ASNum": {
            "type": "object",
            "properties": {
              "key": {
                "default": false,
                "type": "boolean"
              },
              "prompt": {
                "default": "",
                "type": "string"
              },
              "help": {
                "default": "Local AS for BGP global config. Both AsPlain and AsDot formats are supported.",
                "type": "string"
              },
              "islist": {
                "default": false,
                "type": "boolean"
              },
              "cliname": {
                "default": "ASNum",
                "type": "string"
              },
              "argtype": {
                "type": "string"
              },
              "isdefaultset": {
                "default": true,
                "type": "boolean"
              },
              "defaultarg": {
                "default": ""
              }
            }
          }
        }
      },
      "objname": {
        "default": "BGPGlobal", 
        "type": "string", 
        "description": "object name to which references these attributes"
      }
    }
  }
}
